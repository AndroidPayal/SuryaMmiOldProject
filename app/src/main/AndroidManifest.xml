<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.radioknit.suryaelevatorsmmi">
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeviceListActivity" />
        <activity android:name=".WriteModeEnableActivity"/>
        <activity android:name=".MainDisplayActivity" />
        <activity android:name=".CarCallActivity" />
        <activity android:name=".IOIndicatorActivity" />
        <activity android:name=".SetDateTimeActivity" />
        <activity android:name=".ProgramCodeActivity"/>
        <activity android:name=".DisplayPatternActivity"/>
    </application>

</manifest>